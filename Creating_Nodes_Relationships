CREATE (or:station)
SET or.location = Point({latitude: -26.13293, longitude: 28.23157, crs: 'wgs-84'}),
or.name =  "OR Tambo",
or.open = "05:15",
or.close = "20:54";

MATCH
  (a:station),
  (b:station)
WHERE a.name = 'Park' AND b.name = 'Hatfield'
CREATE (a)-[:TO]->(b);

